<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <!-- Id: com.sap.ldi:nexus-settings:1.17.0:LeanDI -->

  <mirrors>
    <mirror>
      <id>mirror1</id>
      <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.milestones/</url>
      <mirrorOf>*,!sapcentral</mirrorOf>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <id>release.build</id>
      <pluginRepositories>
        <pluginRepository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.releases/</url>
        </pluginRepository>
      </pluginRepositories>
      <repositories>
        <repository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.releases/</url>
        </repository>
      </repositories>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
        <maven.color>false</maven.color>
      </properties>
    </profile>
    <profile>
      <id>milestone.build</id>
      <pluginRepositories>
        <pluginRepository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.milestones/</url>
        </pluginRepository>
      </pluginRepositories>
      <repositories>
        <repository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.milestones/</url>
        </repository>
      </repositories>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
        <maven.color>false</maven.color>
      </properties>
    </profile>
    <profile>
      <id>snapshot.build</id>
      <pluginRepositories>
        <pluginRepository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.snapshots/</url>
        </pluginRepository>
      </pluginRepositories>
      <repositories>
        <repository>
          <id>sapcentral</id>
          <url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.snapshots/</url>
        </repository>
      </repositories>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
        <color>false</color>
      </properties>
    </profile>
    <profile>
      <id>sonar</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <sonar.jdbc.url>
          jdbc:mysql://ldisonarci.wdf.sap.corp:3306/sonar?useUnicode=true&amp;characterEncoding=utf8
        </sonar.jdbc.url>
        <sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
        <sonar.jdbc.username>sonar</sonar.jdbc.username>
        <sonar.jdbc.password>sonar</sonar.jdbc.password>
        <sonar.host.url>http://ldisonarci.wdf.sap.corp:8080/sonar</sonar.host.url>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
        <maven.color>false</maven.color>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>snapshot.build</activeProfile>
  </activeProfiles>
  <pluginGroups>
    <pluginGroup>com.sap.ldi</pluginGroup>
    <pluginGroup>fr.jcgay.maven.plugins</pluginGroup>
  </pluginGroups>
</settings>
